{
    "name": "mr-luke/settings-manager",
    "description": "Laravel 5 Package that provides settings manager with type casting.",
    "keywords": ["settings", "manager", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "≈Åukasz Sitnicki",
            "email": "lukasz.sitnicki@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/database": "^9.0|^8.0|^7.0|^6.0|^5.4",
        "illuminate/support": "^9.0|^8.0|^7.0|^6.0|^5.4",
        "mr-luke/configuration": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^2.0|^1.9",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.0|^6.0|^5.0|^4.0|^3.4",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mrluke\\Settings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mrluke\\Settings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrluke\\Settings\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
